	</xs:complexType>
	<xs:complexType name="RozsahDodavky__LocatedType">
		<xs:sequence>
			<xs:element name="informacni-oblast" type="InformacniOblastDodavky__LocatedType">
				<xs:annotation>
					<xs:documentation>Informační oblast dodávky.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="obdobi-sberu" type="ObdobiSberuDodavky__LocatedType">
				<xs:annotation>
					<xs:documentation>Období sběru dodávky (rok).</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="kolo-navrhu" type="KoloNavrhuSR__LocatedType">
				<xs:annotation>
					<xs:documentation>Kolo procesu přípravy státního rozpočtu.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="poskytovatel" type="ResortKod__LocatedType">
				<xs:annotation>
					<xs:documentation>Kód organizační složky státu - poskytovatele prostředků ze státního rozpočtu.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="aktivita" type="AktivitaKod__LocatedType">
				<xs:annotation>
					<xs:documentation>Kód aktivity (programu nebo typu grantových projektů) - uvádí se pro informační oblast CEP.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="predkladatel" type="PredkladatelDodavky__LocatedType">
				<xs:annotation>
					<xs:documentation>Předkladatel dodávky - uvádí se pro informační oblast RIV. Je to ten subjekt, organizační jednotka nebo osoba, která figurovala jako příjemce nebo vykonavatel těch projektů nebo záměrů, z nichž bylo podpořeno dosažení výsledků uvedených v dodávce.     </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="duvernost" type="DuvernostDatDodavky__LocatedType">
				<xs:annotation>
					<xs:documentation>Doplňující určení dodávky z hlediska důvěrnosti obsahu dodávky. Uvádí se pro informační oblasti CEP, CEZ a RIV. Obsahem je jedině kód "neutajovano".</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs="0" name="status" type="StatusDatDodavky__LocatedType">
				<xs:annotation>
					<xs:documentation>Doplňující určení dodávky z hlediska statutu dat dodávky. Uvádí se pro informační oblast CEP. Obsahem je buď kód resene, a potom projekty dodávky nesmějí mít status-teto-faze roven ukoncene nebo zastaveny. Nebo je obsahem kód ukoncene, a potom projekty dodávky musejí mít       status-teto-faze roven ukoncene nebo zastaveny.     </xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attributeGroup ref="LokaceUdaje__Attributes"/>
	</xs:complexType>
	<xs:simpleType name="InformacniOblastDodavky__Type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="CEP">
				<xs:annotation>
					<xs:appinfo source="vklap">CEP - Centrální evidence projektů financovaných ze státního rozpočtu</xs:appinfo>
				</xs:annotation>
			</xs:enumeration>
