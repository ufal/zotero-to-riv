	<!-- Dodávka -->
	<xs:element name="dodavka" type="Dodavka__LocatedType">
		<xs:annotation>
			<xs:documentation>Kořenový element. Reprezentuje celou dodávku.</xs:documentation>
		</xs:annotation>
	</xs:element>
	<xs:complexType name="Dodavka__LocatedType">
		<xs:sequence>
			<xs:element minOccurs="0" name="zahlavi" type="ZahlaviDodavky__LocatedType">
				<xs:annotation>
					<xs:documentation>Informace o dodávce a dodavateli.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="obsah" type="ObsahDodavky__LocatedType">
				<xs:annotation>
					<xs:documentation>Datový obsah dodávky.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
		<xs:attribute name="struktura" type="StrukturaDodavky__Type" use="required">
			<xs:annotation>
				<xs:documentation>Dodavatelská struktura dodávky. Popisuje rozsah sbíraných dat.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attributeGroup ref="LokaceUdaje__Attributes"/>
	</xs:complexType>
	<xs:simpleType name="StrukturaDodavky__Type">
		<xs:restriction base="xs:string">
			<xs:enumeration value="CEP08A"/>
			<xs:enumeration value="CEZ08A"/>
			<xs:enumeration value="RIV08A"/>
			<xs:enumeration value="VES08A"/>
			<xs:enumeration value="SRP08A"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ObsahDodavky__LocatedType">
		<xs:choice minOccurs="0">
			<xs:element maxOccurs="unbounded" ref="projekt"/>
			<xs:element maxOccurs="unbounded" ref="zamer"/>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="vysledek-uplatneny"/>
				<xs:element ref="vysledek-dosud-neuplatneny"/>
			</xs:choice>
			<xs:choice maxOccurs="unbounded">
				<xs:element ref="verejna-soutez-vyhlasena"/>
				<xs:element ref="verejna-soutez-vyhodnocena"/>
			</xs:choice>
			<xs:element maxOccurs="unbounded" ref="oblast"/>
		</xs:choice>
		<xs:attributeGroup ref="LokaceUdaje__Attributes"/>
	</xs:complexType>
